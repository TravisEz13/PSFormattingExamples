<?xml version="1.0" encoding="utf-8"?>
<Configuration>
  <ViewDefinitions>
    <View>
      <Name>exampleA</Name>
      <ViewSelectedBy>
        <TypeName>b</TypeName>
      </ViewSelectedBy>
      <!--GroupBy>
        <PropertyName>PSParentPath</PropertyName>
      </GroupBy -->

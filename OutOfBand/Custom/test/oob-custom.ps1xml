      </TableControl>
    </View>
        <View>
            <Name>c</Name>
            <OutOfBand />
            <ViewSelectedBy>
                <TypeName>CCustom</TypeName>
            </ViewSelectedBy>
            <CustomControl>
                <CustomEntries>
                    <CustomEntry>
                       <CustomItem>
                            <ExpressionBinding>
                                <ScriptBlock>$_.Render()</ScriptBlock>
                            </ExpressionBinding>
                        </CustomItem>
                    </CustomEntry>
                </CustomEntries>
            </CustomControl>
        </View>
      </ViewDefinitions>

              <TableColumnItem>
                <ScriptBlock>$_.s</ScriptBlock>
              </TableColumnItem>
              <TableColumnItem>
                <ScriptBlock>$_.i</ScriptBlock>
              </TableColumnItem>
            </TableColumnItems>
